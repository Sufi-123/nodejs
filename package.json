    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "http-status-codes": "^1.3.2",
    "joi": "^14.3.1",
    "knex": "^0.17.6",
    "lodash": "^4.17.11",
    "pg": "^7.11.0",
    "swagger-jsdoc": "^3.2.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
